{
  "name": "adonis-react-auth",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application with auth boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "front-start": "node scripts/start.js",
    "front-test": "node scripts/test.js",
    "front-build": "node scripts/build.js"
  },
  "keywords": [
    "adonisjs",
