    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^6.0.1",
    "redux": "^4.0.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
